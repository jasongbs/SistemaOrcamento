<h1>Formulário de Orcamentos</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Orcamentos</th>
      <th scope="col">Fornecedor</th>
      <th scope="col">Descrição</th>
      <th scope="col">Data Solicitação</th>
      <th scope="col">Data Limite</th>
      <th scope="col">Status</th>
    
    </tr>
    {{#each orcamentos}}
     <td>{{id}}</td>
    <td>{{idFornecedor}}</td>
     <td>{{descricao}}</td>
     <td>{{dataSolicitacao}}</td>
      <td>{{dataLimite}}</td>
    <td>{{statusOrcamento}}</th>
    
      
     </tr>
{{/each}}

  </tbody>
</table>